{
  "name": "orchestral",
  "version": "0.1.0",
  "description": "Agentic Software Engineering MVP - 4 agents orchestrate code generation with human oversight",
  "type": "module",
  "main": "dist/cli/index.js",
  "bin": {
    "orchestral": "dist/cli/index.js"
  },
  "scripts": {
    "build": "tsc && npm run copy-assets",
    "copy-assets": "mkdir -p dist/server/public && cp -r src/server/public/* dist/server/public/ && mkdir -p dist/templates && cp -r templates/* dist/templates/",
    "dev": "tsx src/cli/index.ts",
    "start": "node dist/cli/index.js",
    "watch": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "ai",
    "agents",
    "code-generation",
    "orchestration"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "chalk": "^5.3.0",
    "chokidar": "^3.5.3",
    "commander": "^12.0.0",
    "express": "^4.18.2",
    "open": "^10.0.0",
    "socket.io": "^4.7.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@vitest/coverage-v8": "^4.0.18",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0",
    "vitest": "^4.0.18"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
